<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="EndToEndFlowSuite" verbose="1" parallel="classes">
	
	<test name="DemoQATestOnChrome">
		<parameter name="browserName" value="Chrome" />
		<parameter name="url"
			value="https://demoqa.com/" />
		<classes>
			<class name="com.demoqa.tests.SubmitUserFormTest" />
			<class name="com.demoqa.tests.UserDataTest" />
		</classes>
	</test>
	<test name="DemoQATestOnEdge">
		<parameter name="browserName" value="Edge" />
		<parameter name="url"
			value="https://demoqa.com/" />
		<classes>
			<class name="com.demoqa.tests.SubmitUserFormTest" />
			<class name="com.demoqa.tests.UserDataTest" />
		</classes>
	</test>
</suite>